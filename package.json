{
  "name": "foldertree-pro",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:react\" \"npm:dev:electron\"",
    "dev:react": "cd frontend && npm run dev",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "build": "npm run build:react && electron-builder",
    "build:react": "cd frontend && npm run build"
  },
  "build": {
    "appId": "com.yourcompany.foldertree",
    "files": [
      "frontend/out/**",
      "public/electron/*main.js",
      "backend/src/**",
      "backend/folder_tree.json",
      "backend/generated_folders/**"
    ],
    "directories": {
      "buildResources": "public/electron/assets"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@huggingface/inference": "^4.5.1",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "graph.js": "^1.21.1",
    "lucide-react": "^0.525.0",
    "next": "^16.1.1-canary.10",
    "openai": "^5.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "uuid": "^11.1.0",
    "wait-on": "^7.0.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/next": "^8.0.7",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "type": "commonjs"
}
